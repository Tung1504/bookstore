@model IEnumerable<bookstore.Models.Order>
@{
    ViewBag.Title = "PurchasedSuccess";
}

<h2>Congratuation</h2>
<div class="alert alert-success">
    Your purchased has placed successfully
</div>
<table>
    <tr>
        <th>Order Number</th>
        <th>Order time</th>
        <th>Order Adress</th>
        <th>Order Status</th>
        <th>Total Cost</th>
    </tr>
    @foreach(var order in Model)
    {
    <tr>
        <td>@order.order_number</td>
        <td>@order.date</td>
        <td>@order.delivery_location</td>
        <td>@order.status</td>
        <td>@order.total_price</td>
    </tr>
    }
    
</table>